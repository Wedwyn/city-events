{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
      ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-underscore-dangle": [
            2,
            {
              "allow": [
                "__filename",
                "__dirname"
              ]
            }
          ],
          "import/extensions": 0
    }
}
